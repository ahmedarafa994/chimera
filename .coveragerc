[run]
branch = True
source = 
    backend-api/app
    meta_prompter
omit =
    */tests/*
    */__pycache__/*
    */migrations/*
    */.venv/*
    */venv/*
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

fail_under = 80
show_missing = True
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml